<%unless flights.empty?%>
    <%= form_with url: "/bookings/new", amethod: "GET" do |f|%>
        <%flights.each do |flight|%>
                <%= f.radio_button :flight_id, flight.id %>
                <%= f.hidden_field :passenger_count, value: passenger_count%>
                <%= render 'flight', flight: flight %>
        <% end %>
        <%= f.submit "Submit"%>
    <% end %>
<% end %>